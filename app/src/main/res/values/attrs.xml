<resources>

    <attr name="fontStyle" format="enum">
        <enum name="proxima_nova_normal" value="0" />
        <enum name="proxima_nova_bold" value="1" />
        <enum name="proxima_nova_bolditalic" value="2" />
        <enum name="proxima_nova_extrabold" value="3" />
        <enum name="proxima_nova_regular" value="4" />
        <enum name="proxima_nova_reg_italic" value = "5"/>
        <enum name="proxima_nova_regular_italic" value = "6"/>
        <enum name="helvetica_neue_light" value = "7"/>
    </attr>

    <declare-styleable name="TypefacedTextView">
        <attr name="fontStyle" />
    </declare-styleable>

</resources>
